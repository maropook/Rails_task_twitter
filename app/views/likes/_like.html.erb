<% if current_user.already_liked?(post) %>
<%= link_to image_tag("heart_brown.png", class:"like"),  post_like_path(post,current_user),method: :delete, remote: true%>
<% else %>
<%= link_to image_tag("heart_white.png", class:"like"),post_likes_path(post),method: :post, remote: true%>
<% end %>
<%= post.liked_users.count %>
